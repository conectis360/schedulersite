{
    "manifest_version": 2,
    "name": "Schedulersite",
    "version": "1.0",
    "description": "Block sites using a schedule for you to see when you want/need.",
  
    "permissions": [
      "webRequest",        
      "webRequestBlocking",
      "<all_urls>"         
    ],
  
    "background": {
      "scripts": ["background.js"]
    }
  }